{namespace todomvc.templates}

/**
 * @param model
 */
{template .todoItem}
  <li>
    <div class="view">
      <input class="toggle" type="checkbox">
      <label>{$model['title']}</label>
      <button class="destroy"></button>
    </div>
    <input class="edit" type="text" value="">
  </li>
{/template}

/**
 * @param left
 */
{template .itemsLeft}
  <strong>{$left}</strong> item{if $left != 1}s{/if} left
{/template}
